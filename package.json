{
  "name": "@kosatyi/rollup",
  "description": "Rollup plugins bundle",
  "homepage": "https://github.com/kosatyi/rollup",
  "version": "0.0.25",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "version": "npm run build",
    "prepublishOnly": "npm i && rollup -c",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "@rollup/plugin-babel": "6.x",
    "@rollup/plugin-commonjs": ">= 28.0.1",
    "@rollup/plugin-json": "6.x",
    "@rollup/plugin-node-resolve": "15.x",
    "@rollup/plugin-terser": "0.x",
    "@rollup/plugin-typescript": "11.x",
    "@rollup/pluginutils": "5.x",
    "globals": "15.x",
    "rollup": "4.x",
    "rollup-plugin-cleanup": "3.x",
    "rollup-plugin-copy": "3.x",
    "rollup-plugin-execute": "1.x",
    "rollup-plugin-ignore": "1.x",
    "rollup-plugin-scss": "4.x",
    "rollup-plugin-watch": "1.x",
    "sass": "1.77.4",
    "tslib": "2.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kosatyi/rollup.git"
  },
  "keywords": [
    "rollup",
    "utils"
  ],
  "author": "Stepan Kosatyi (https://kosatyi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kosatyi/rollup/issues"
  }
}
